        <img src="{{ asset('assets/images/loader.svg') }}" alt="loader">
    </div>

    <div class="page">
        <div class="page-main">

            <!--aside open-->
            {% block sidenav %}
				{{ parent() }}
			{% endblock %}
            <!--aside closed-->

            <div class="app-content main-content">
                <div class="side-app">

                    <!--app header-->                   
                    {% block topnav %}
                        {{ parent() }}
                    {% endblock %}
                    <!--/app header-->

                    <!--Page header-->
                    <div class="page-header">
                        <div class="page-leftheader">
                            <h4 class="page-title">Client Dashboard</h4>
                        </div>
                        <div class="page-rightheader ms-auto d-lg-flex d-none hide-me">
                            <div class="ms-5 mb-0">
                                <a class="btn btn-white date-range-btn" href="javascript:void(0)" id="daterange-btn">
