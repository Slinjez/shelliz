{% extends "layouts/portallayout.html.twig" %}

{% block head %}
	{{ parent() }}
	{# <link href="{{ asset('assets/plugins/pagination/dist/bs-pagination.min.css') }}" rel="stylesheet"/> #}
	<link
	href="{{ asset('assets/plugins/datetime/build/jquery.datetimepicker.min.css') }}" rel="stylesheet"/>


	<!-- Time picker css -->
	<link
	href="{{ asset('assets/plugins/time-picker/jquery.timepicker.css') }}" rel="stylesheet"/>

	<!-- Date Picker css -->
	<link href="{{ asset('assets/plugins/date-picker/date-picker.css') }}" rel="stylesheet"/>

	<style>
		.hide-me {
			display: none;
		}
	</style>
{% endblock %}
{% block mainContent %}
	<body
		class="app sidebar-mini light-mode default-sidebar">

		<!---Global-loader-->
		<div id="global-loader">
			<img src="{{ asset('assets/images/loader.svg') }}" alt="loader">
		</div>

		<div class="page">
			<div
				class="page-main">

				<!--aside open-->
				{% block sidenav %}
					{{ parent() }}
				{% endblock %}
				<!--aside closed-->

				<div class="app-content main-content">
					<div
						class="side-app">

						<!--app header-->
						{% block topnav %}
							{{ parent() }}
						{% endblock %}
						<!--/app header-->

						<!--Row-->
						<div class="row">
							<div
								class="side-app">

								<!--Page header-->
								<div class="page-header">
									<div class="page-leftheader">
										<h4 class="page-title">{{page_name}}</h4>
									</div>
									<div class="page-rightheader ms-auto d-lg-flex d-none">
										<ol class="breadcrumb">
											<li class="breadcrumb-item">
												<a href="/client-dash" class="d-flex">
													<svg class="svg-icon" xmlns="http://www.w3.org/2000/svg" height="24" viewbox="0 0 24 24" width="24"><path d="M0 0h24v24H0V0z" fill="none"/><path d="M12 3L2 12h3v8h6v-6h2v6h6v-8h3L12 3zm5 15h-2v-6H9v6H7v-7.81l5-4.5 5 4.5V18z"/><path d="M7 10.19V18h2v-6h6v6h2v-7.81l-5-4.5z" opacity=".3"/></svg>
													<span class="breadcrumb-icon">
														Home</span>
												</a>
											</li>
											<li class="breadcrumb-item">
												<a href="javascript:void(0)">{{page_name}}</a>
											</li>
										</ol>
									</div>
								</div>
								<!--End Page header-->

								<!-- Row -->
								<div class="row">
									<div class="col-lg-12">
										<div id="preload" class="progress progress-md mb-3">
												<div class="progress-bar progress-bar-indeterminate bg-pink" ></div>
											</div>
									</div>
									<div class="col-xl-12 col-lg-12 col-md-12 row hide-me" id="result-view">

										<div class="paginate 1">
											<div class="items row" id="placement"></div>
											<div class="pager pagination ">
												<div class="firstPage page-item">&laquo;</div>
												<div class="page-item previousPage page-item">&lsaquo;</div>
												<div class="page-item pageNumbers page-item"></div>
												<div class="page-item nextPage page-item">&rsaquo;</div>
												<div class="page-item lastPage page-item">&raquo;</div>
											</div>
										</div>

									</div>
								</div>
							</div>
						</div>

					</div>
				</div>
				<!-- end app-content-->
			</div>

			<!--Footer-->

			<!-- End Footer-->

		</div>

		<!-- Back to top -->
		<a href="#top" id="back-to-top">
			<svg class="svg-icon" xmlns="http://www.w3.org/2000/svg" height="24" viewbox="0 0 24 24" width="24"><path d="M0 0h24v24H0V0z" fill="none"/><path d="M4 12l1.41 1.41L11 7.83V20h2V7.83l5.58 5.59L20 12l-8-8-8 8z"/></svg>
		</a>


		<div class="modal fade" id="request-callback" aria-hidden="true" style="display: none;">
			<div class="modal-dialog" role="document">
				<div class="modal-content modal-content-demo">
					<div class="modal-header">
						<h6 class="modal-title">Request Call Back</h6>
						<button aria-label="Close" class="btn-close" data-bs-dismiss="modal" type="button">
							<span aria-hidden="true">×</span>
						</button>
					</div>
					<form id="call-back-form">
						<div class="modal-body">
							<input type="hidden" id="call-back-product-id">
							<div class="row">
								<div class="col-md-12">
									<div class="removeMessages-modal"></div>
									<div class="ajaxloader-modal"></div>
								</div>
							</div>
							<div class="mb-4">
								<div class="progress progress-md mb-3 hide-me bot-load-modal">
									<div class="progress-bar progress-bar-indeterminate bg-cyan"></div>
								</div>
							</div>
							<div class="row">

								<div class="col-sm-6 col-md-12">
									<div class="mb-3">
										<label class="form-label">Select Date and Time<small></small>
										</label>
										<div class="wd-200 mg-b-30">
											<div id="show_inline" style="display:flex">
												<input type="text" id="datetimepicker10"/>
											</div>
										</div>
									</div>
								</div>

							</div>

						</div>
						<div class="modal-footer">
							<button id="form-submitter-modal" class="btn btn-indigo" type="submit">Save</button>
							<button class="btn btn-light" data-bs-dismiss="modal" type="button">Close</button>
						</div>
					</form>
				</div>
			</div>
		</div>


		<div class="modal fade" id="bank-preview" aria-hidden="true" style="display: none;">
			<div class="modal-dialog" role="document">
				<div class="modal-content modal-content-demo">
					<div class="modal-header">
						<h6 class="modal-title">Bank Preview</h6>
						<button aria-label="Close" class="btn-close" data-bs-dismiss="modal" type="button">
							<span aria-hidden="true">×</span>
						</button>
					</div>
					<form id="bank-form-update">
						<div class="modal-body">
							<h6>Acc. (<span id="title-bank-name"></span>) Details</h6>
							<div class="row">
								<div class="col-md-12">
									<div class="removeMessages-modal"></div>
									<div class="ajaxloader-modal"></div>
								</div>
							</div>
							<div class="mb-4">
								<div class="progress progress-md mb-3 hide-me bot-load-modal">
									<div class="progress-bar progress-bar-indeterminate bg-cyan"></div>
								</div>
							</div>
							<div class="row">
								<div class="col-sm-12 col-md-12">
									<input type="hidden" id="bank-id-up">
									<div class="mb-3">
										<label class="form-label">Bank Name</label>
										<input id="bank-name-up" type="text" class="form-control" placeholder="Bank Name">
									</div>
								</div>
								<div class="col-md-6">
									<div class="mb-3">
										<label class="form-label">Bank Branch</label>
										<input id="bank-branch-up" type="text" class="form-control" placeholder="Branch">
									</div>
								</div>
								<div class="col-sm-6 col-md-6">
									<div class="mb-3">
										<label class="form-label">Account Number<small></small>
										</label>
										<input id="account-number-up" type="number" class="form-control" placeholder="Number">
									</div>
								</div>
							</div>

						</div>
						<div class="modal-footer">
							<button id="form-submitter-update-modal" class="btn btn-indigo" type="submit">Update</button>
							<button class="btn btn-light" data-bs-dismiss="modal" type="button">Close</button>
						</div>
					</form>
				</div>
			</div>
		</div>
	{% endblock %}
	{% block pageScripts %}

		<!-- function js-->
		<script src="{{ asset('assets/js/moment.js') }}"></script>
		<script src="{{ asset('assets/plugins/datetime/build/jquery.datetimepicker.full.min.js') }}"></script>
		<script src="{{ asset('assets/plugins/pagination/paginga.jquery.min.js') }}"></script>


		{# <!-- Timepicker js -->
		    <script src="{{ asset('assets/plugins/time-picker/jquery.timepicker.js') }}"></script>
		    <script src="{{ asset('assets/plugins/time-picker/toggles.min.js') }}"></script>
		
		    <!-- Datepicker js -->
		    <script src="{{ asset('assets/plugins/date-picker/date-picker.js') }}"></script>
		    <script src="{{ asset('assets/plugins/date-picker/jquery-ui.js') }}"></script>
		    <script src="{{ asset('assets/plugins/input-mask/jquery.maskedinput.js') }}"></script> #}

		<script src="{{ asset('assets/func/products/listing.js') }}"></script>
	{% endblock %}
	
